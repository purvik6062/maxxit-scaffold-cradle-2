{
    "name": "@cradle/wallet-auth",
    "version": "0.1.0",
    "description": "Wallet authentication with RainbowKit and wagmi for Web3 applications",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@rainbow-me/rainbowkit": "^2.2.0",
        "@tanstack/react-query": "^5.62.0",
        "viem": "^2.21.0",
        "wagmi": "^2.14.0"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "files": [
        "dist",
        "src"
    ],
    "keywords": [
        "wallet",
        "rainbowkit",
        "wagmi",
        "web3",
        "ethereum",
        "arbitrum",
        "authentication"
    ],
    "license": "MIT"
}