{
    "name": "@cradle/maxxit-lazy-trader",
    "version": "0.1.0",
    "description": "Maxxit Lazy Trader API integration - connect and message your Lazy Trader agent",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {},
    "peerDependencies": {
        "react": "^18.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "react": "^18.2.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "files": [
        "dist",
        "src"
    ],
    "keywords": [
        "maxxit",
        "lazy-trader",
        "trading",
        "api",
        "agent",
        "web3"
    ],
    "license": "MIT"
}
